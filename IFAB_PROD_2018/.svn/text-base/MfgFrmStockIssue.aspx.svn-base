                                                        <asp:HiddenField ID="ID" runat="server" Value='<%# Bind("ID_AutoNo") %>' />
                                                        <asp:Label ID="lblSuppName" runat="server" Text='<%# Bind("Issue_Date","{0:dd-MMM-yyyy}") %>' />
