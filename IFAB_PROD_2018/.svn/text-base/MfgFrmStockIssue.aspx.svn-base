                                                <asp:ObjectDataSource ID="ObjSE" runat="server" SelectMethod="GetSECobmo" TypeName="SupplierDB">
                                                </asp:ObjectDataSource>
                                            </td>
                                        </tr>
                                    </table>
                                </center>
                                <%--  </asp:Panel>--%>
                                <br />
