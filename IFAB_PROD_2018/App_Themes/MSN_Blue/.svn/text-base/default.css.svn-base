/* Common tags                 */
/*-----------------------------*/
cap
{
	vertical-align: middle;
	text-align: center;
}
body
{
	font-family: Verdana;
	font-size: small;
	color: #000000;
	margin: 0 auto;
	padding: 0;
	margin-right: 30;
	text-align: left;
	width: 1024px;
	height: 768px;
}
.ContentPlaceHolder
{
	background-color: #79afe1;
}
.Form
{
	background-color: #00016f; /*A8B6DD;*/
}

.Body
{
	background-color: #A8B6DD; /*A8B6DD;*/
}

.headingTxt
{
	font: Arial;
	color: #005C99;
	font-size: large;
	margin: 10px 0px 10px 0px;
	font-style: italic;
	width: 660px;
}

.headingTxtRsz
{
	font: Arial;
	color: #005C99;
	font-size: large;
	margin: 10px 0px 10px 0px;
	font-style: italic;
	width: 325px;
}
.MenuLeft
{
	width: 185px;
	background-color: #005C99;
	text-align: left;
	color: White;
	height: 20px;
}

A:link
{
	color: #004040;
	text-decoration: none;
}
A:visited
{
	color: #004040;
	text-decoration: none;
}
A:active
{
	color: #004040;
	cursor: hand;
	text-decoration: none;
}
A:hover
{
	color: blue;
	cursor: hand;
	text-decoration: "underline";
}

A.bodyLink:link
{
	color: #0053A9;
	text-decoration: none;
	font-weight: bold;
}
A.bodyLink:visited
{
	color: #0053A9;
	text-decoration: none;
	font-weight: bold;
}
A.bodyLink:active
{
	color: #0053A9;
	cursor: hand;
	text-decoration: none;
	font-weight: bold;
}
A.bodyLink:hover
{
	color: #911B12;
	cursor: hand;
	text-decoration: "underline";
	font-weight: bold;
}

p
{
	line-height: 1.4em;
}

.title
{
	text-transform: uppercase;
	text-align: center;
	font-family: verdana;
	font-size: large;
	font-weight: bold;
	color: #0053A9;
}

hr
{
	border: 0;
	border-top: 2px solid #41519A;
	height: 2px;
}

img
{
	border-width: 0;
}

ul
{
	list-style-image: url(Images/bullet.jpg);
	list-style-position: outside;
	list-style-type: disc;
	color: #000000;
	font-family: verdana;
}

.menutextindent
{
	font-size: x-small;
}

/* Headings                    */
/*-----------------------------*/
h1
{
	font-size: large;
	color: #6B7EBF;
}

h2
{
	font-family: Verdana;
	font-size: medium;
	margin-top: 30;
	color: #6B7EBF;
	margin-bottom: -15;
}

h3
{
	font-family: Verdana;
	font-size: small;
	margin-bottom: -15;
	color: #6B7EBF;
	padding-left: 15;
}

h1, h2, h3, h4
{
	margin: 0;
	font-family: Verdana;
}

/* Tables                      */
/*-----------------------------*/
table
{
	font-size: 1em;
}

table.header
{
	background-color: #0E5097;
}

td.logo
{
	text-align: left;
	width: 184px;
}

td.title
{
	text-align: center;
	font-family: verdana;
	font-size: x-large;
	font-weight: bolder;
	color: #FFFFFF;
}

td.headerbar
{
	background-image: url(Images/bar.jpg);
	text-align: right;
	height: 24px;
}

td.menu
{
	background-color: #0E5097;
	width: 184px;
	height: 500px;
	vertical-align: top;
	margin-left: 0px;
	letter-spacing: 0px;
}

td.footer
{
	margin-left: 30;
	font-family: Verdana;
	font-size: xx-small;
	font-weight: normal;
	color: #0E5097;
	text-align: center;
}

.mypageSkin
{
	font-family: verdana;
	font-weight: bold;
	font-size: 10pt;
	color: #ff9900;
}
.mypageSkin A
{
	font-family: verdana;
	font-size: 9pt;
	text-decoration: none;
	color: #0000ff;
}
.mypager
{
	font-family: verdana;
	font-weight: bold;
	font-size: 11pt;
	color: #ff9900;
}
.mypager A
{
	font-family: verdana;
	font-size: 9pt;
	text-decoration: none;
	color: #0000ff;
}
.btnStyle
{
	margin: 0px 2px 0px 3px;
}
.buttons
{
	font-family: "Trebuchet MS";
	color: #FFF;
	font-weight: bold;
	background-image: url(Images/button.jpg);
	background-repeat: repeat-x;
	border-style: solid;
	border: 3px;
	width: auto;
	height: 25px;
}
.buttonshover
{
	color: #000000;
	font-family: "Trebuchet MS";
	font-weight: bold;
	background-image: url(Images/button_hover.jpg);
	background-repeat: repeat-x;
	border-style: solid;
	border: 3px;
	width: auto;
	height: 25px;
}
